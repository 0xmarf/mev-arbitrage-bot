{
  "name": "mev-bot",
  "version": "1.0.0",
  "description": "MEV arbitrage bot",
  "main": "dist/src/index.js",
  "type": "module",
  "scripts": {
    "start": "node build/index.js",
    "start:ws": "node build/index.websocket.js",
    "start:mevshare": "node build/index.mevshare.js",
    "test": "NODE_OPTIONS='--experimental-vm-modules --no-warnings' hardhat test",
    "test:mevshare": "NODE_OPTIONS='--experimental-vm-modules --no-warnings' hardhat test test/mevshare/*.test.ts",
    "build": "tsc",
    "build:ws": "tsc -p tsconfig.websocket.json",
    "build:all": "npm run build && npm run build:ws",
    "prepare": "npm run build",
    "lint": "npx eslint src",
    "compile": "tsc",
    "dev": "npm run build && npm run start",
    "dev:ws": "npm run build:ws && npm run start:ws",
    "monitor": "NODE_ENV=production node build/index.js",
    "monitor:ws": "NODE_ENV=production node build/index.websocket.js"
  },
  "dependencies": {
    "@ethersproject/providers": "^5.7.2",
    "@ethersproject/wallet": "^5.7.0",
    "@flashbots/ethers-provider-bundle": "^0.6.2",
    "@flashbots/matchmaker-ts": "^0.7.6",
    "@types/node-fetch": "^2.6.12",
    "axios": "^1.7.9",
    "dotenv": "^16.4.1",
    "ethers": "^6.13.5",
    "lodash": "^4.17.21",
    "node-fetch": "^2.7.0",
    "p-limit": "^5.0.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "winston": "^3.11.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@aave/core-v3": "^1.19.3",
    "@babel/core": "^7.26.8",
    "@babel/plugin-syntax-flow": "^7.26.0",
    "@babel/plugin-transform-flow-strip-types": "^7.26.5",
    "@babel/preset-env": "^7.26.8",
    "@babel/preset-typescript": "^7.26.0",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.0",
    "@openzeppelin/contracts": "^4.9.6",
    "@typechain/ethers-v6": "^0.5.0",
    "@typechain/hardhat": "^9.0.0",
    "@types/chai": "^4.2.0",
    "@types/jasmine": "^3.6.2",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.14.202",
    "@types/mocha": ">=9.1.0",
    "@types/node": ">=16.0.0",
    "@types/p-limit": "^2.1.0",
    "@types/sinon": "^17.0.3",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "babel-jest": "^29.7.0",
    "chai": "^4.2.0",
    "eslint": "^7.15.0",
    "hardhat": "^2.19.5",
    "hardhat-gas-reporter": "^1.0.8",
    "jasmine": "^3.6.3",
    "jest": "^29.7.0",
    "mocha": "^11.1.0",
    "nyc": "^15.1.0",
    "sinon": "^19.0.2",
    "solidity-coverage": "^0.8.0",
    "ts-jest": "^29.2.5",
    "ts-node": ">=8.0.0",
    "typechain": "^8.3.0",
    "typescript": ">=4.5.0"
  }
}
